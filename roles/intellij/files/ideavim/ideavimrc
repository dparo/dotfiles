set idearefactormode=keep
set ideajoin
set surround
set NERDTree

set smartcase
set ignorecase

set clipboard+=unnamedplus

set selection=exclusive

let mapleader = " "

nmap <leader>ff :action GotoFile<CR>
nmap <M-x> :action GotoAction<CR>
nmap <C-`> :action Terminal<CR>
nmap <F4> :action Terminal<CR>
