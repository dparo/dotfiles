include /usr/share/doc/newsboat/contrib/colorschemes/inkpot

browser "~/.local/share/bin/newsboat-open-url"

notify-always no
notify-program "dunstify"
notify-format "%d new articles (%n unread articles, %f unread feeds)"

# Reload FEEDS on startup
auto-reload yes
# Reload FEEDS every 10 minutes
reload-time 10
